{
  "name": "nuxt3-ele-tpl",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_NODE_ENV=dev nuxi dev",
    "test": "cross-env NUXT_NODE_ENV=test nuxi dev",
    "devBuild": "cross-env NUXT_NODE_ENV=dev nuxi build",
    "testBuild": "cross-env NUXT_NODE_ENV=test nuxi build",
    "build": "cross-env NUXT_NODE_ENV=prod nuxi build",
    "generate": "cross-env NUXT_NODE_ENV=prod nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "lint": "eslint . --ext .ts,.tsx,.vue --cache --color",
    "lintfix": "eslint . --ext .ts,.tsx,.vue --cache --color --fix",
    "commit": "git add . && git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^1.3.1",
    "lint-staged": "^11.1.2",
    "nuxt": "3.0.0-rc.11",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@nuxtjs/i18n": "8.0.0-beta.2",
    "element-plus": "^2.2.19"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "npm run lint",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
